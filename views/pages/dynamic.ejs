<!doctype html>
<html>
  <head>
    <link
      href="https://cdn.jsdelivr.net/npm/tailwindcss@2.2.19/dist/tailwind.min.css"
      rel="stylesheet"
    />
    <meta charset="utf/8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>Simple Plugin Example</title>
  </head>
  <script type="module">
    import { enableRouter } from '/js/banno-plugin-framework-bridge.js';
    enableRouter();
  </script>
  <body class="min-h-screen flex items-center justify-center flex-col">
    <div>Anchors using: <b>@jack-henry/banno-plugin-framework-bridge</b>
    </div>
    <br />
    <ul>
      <li>
        <a href="<%= url %>">normal</a>
      </li>
      <li>
        <a target="_top" href="<%= url %>">target="_top"</a>
      </li>
      <li>
        <a target="_blank" href="<%= url %>">target="_blank"</a>
      </li>
    </ul>
  </body>
</html>
